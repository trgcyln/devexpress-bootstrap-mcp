{
  "name": "devexpress-bootstrap-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for crawling and indexing DevExpress ASP.NET Bootstrap documentation",
  "repository": {
    "type": "git",
    "url": "https://github.com/trgcyln/devexpress-bootstrap-mcp.git"
  },
  "homepage": "https://github.com/trgcyln/devexpress-bootstrap-mcp#readme",
  "type": "module",
  "main": "dist/server.js",
  "bin": {
