    "crawl": "node dist/crawl.js",
    "dev": "tsc && node dist/server.js",
    "watch": "tsc --watch"
  },
  "keywords": [
    "mcp",
    "devexpress",
    "bootstrap",
    "asp.net",
    "documentation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "cheerio": "^1.0.0",
    "minisearch": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
